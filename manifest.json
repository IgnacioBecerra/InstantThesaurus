{
	"manifest_version": 2,
	"name": "Synonym Searcher",
	"version": "0.1",

	"content_scripts": [
          {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["jquery-3.2.1.min.js"]
	  }
	],

	"background": {
	  "scripts": ["background.js", "popup.js"]
	},

	"browser_action": {
	  "default_icon": "icon.png",
	  "default_popup": "popup.html"
	}
}
